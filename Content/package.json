{
  "version": "1.0.1",
  "name": "dotnetvuetemplate",
  "author": {
    "name": "Jaxel Rojas"
  },
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode development && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "dev:vendor": "webpack --config webpack.config.vendor.js --progress --mode development",
    "build": "npm run build:vendor && webpack --config webpack.config.js --mode production",
    "build:vendor": "webpack --config webpack.config.vendor.js --progress --mode production",
    "install": "npm run dev:vendor"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "aspnet-webpack": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^0.5.9",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-command": "^0.2.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-merge": "^4.1.0"
  }
}
